{
  "name": "@polkadot-api/sdk-ink",
  "version": "0.6.2",
  "sideEffects": false,
  "author": "Victor Oliva (https://github.com/voliva)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polkadot-api/papi-sdks.git"
  },
  "exports": {
    ".": {
      "node": {
        "production": {
          "import": "./dist/esm/src/index.mjs",
          "require": "./dist/min/index.js",
          "default": "./dist/index.js"
        },
        "import": "./dist/esm/src/index.mjs",
        "require": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "module": "./dist/esm/src/index.mjs",
      "import": "./dist/esm/src/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/esm/src/index.mjs",
  "browser": "./dist/esm/src/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "papi generate && ([ -f .papi/descriptors/dist/index.mjs ] && mv .papi/descriptors/dist/index.mjs .papi/descriptors/dist/index.js || [ 0 ]) && tsc --noEmit && rollup -c ../../rollup.config.js",
    "lint": "prettier --check README.md \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "format": "prettier --write README.md \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "prepack": "pnpm run build"
  },
  "license": "MIT",
  "dependencies": {
    "@ethereumjs/rlp": "^10.1.1",
    "@polkadot-api/common-sdk-utils": "workspace:*",
    "@polkadot-api/substrate-bindings": "^0.19.0",
    "abitype": "^1.2.3",
    "polkadot-api": "^2.0.0-rc.2",
    "viem": "^2.46.2"
  },
  "peerDependencies": {
    "@polkadot-api/ink-contracts": ">=0.5.0",
    "polkadot-api": ">=2.0.0-rc.2",
    "rxjs": ">=7.8.0"
  },
  "devDependencies": {
    "@polkadot-api/ink-contracts": "^0.5.1",
    "polkadot-api": "^2.0.0-rc.2",
    "rxjs": "^7.8.2"
  }
}
